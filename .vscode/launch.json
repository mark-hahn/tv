{
    "version": "0.2.0",
    "configurations": [
      {
          "type": "chrome",
          "request": "launch",
          "name": "Launch Chrome against localhost",
          "url": "http://localhost:5173",
          "webRoot": "${workspaceFolder}"
      },      
      {
          "type": "chrome",
          "request": "attach",
          "name": "attach Chrome to localhost",
